{
    "manifest_version": 2,
  
    "name": "Malicious Link Checker",
    "description": "Checks online hyperlinks for malicious content.",
    "version": "1.0",
  
    "icons": {
      "48": "icon1.png"
    },
  
    "permissions": ["tabs", "<all_urls>"],
  
    "browser_action": {
      "default_icon": "icon1.png",
      "default_popup": "popup.html"
    }
  }